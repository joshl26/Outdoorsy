{
  "name": "outdoorsy",
  "version": "1.0.0",
  "description": "A modern campground review and discovery platform",
  "author": "Joshua Lehman <your.email@example.com>",
  "packageManager": "pnpm@8.15.0",
  "homepage": "/outdoorsy/",
  "demo": "https://joshlehman.ca/outdoorsy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshl26/Outdoorsy.git"
  },
  "bugs": {
    "url": "https://github.com/joshl26/Outdoorsy/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "main": "app.js",
  "scripts": {
    "start-windows": "set PORT=3053 && nodemon -e js,ejs,yaml,json server.js",
    "start": "cross-env PORT=3053 node server.js",
    "start:dev": "cross-env PORT=3053 nodemon -e js,ejs,yaml,json server.js",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern node_modules/",
    "format:check": "prettier --check \"**/*.{js,json,md,yaml,yml}\"",
    "validate": "npm run lint && npm run format:check && npm run test",
    "prepare": "husky install",
    "seed": "node seeds/index.js",
    "db:backup": "node scripts/backup-db.js",
    "security:audit": "npm audit --production",
    "security:fix": "npm audit fix",
    "docs:generate": "jsdoc2md **/*.js > docs/API.md",
    "docker:build": "docker build -t outdoorsy:latest .",
    "docker:run": "docker run -p 3053:3053 outdoorsy:latest",
    "precommit": "lint-staged",
    "migrate": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "migrate:status": "node scripts/migrate.js status",
    "migrate:create": "node scripts/migrate.js create"
  },
  "keywords": [
    "campground",
    "camping",
    "outdoors",
    "review",
    "travel",
    "nature",
    "express",
    "mongodb",
    "nodejs"
  ],
  "license": "MIT",
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.16.0",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "ejs-mate": "^4.0.0",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.3.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "method-override": "^3.0.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0",
    "sanitize-html": "^2.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "^5.0.4",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.11",
    "@types/method-override": "^3.0.0",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^2.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^8.0.0",
    "lint-staged": "^15.5.2",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "swagger-inline": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "app.js",
      "routes/",
      "controllers/",
      "models/",
      "middleware/",
      "config/",
      "utils/"
    ],
    "ext": "js,ejs,yaml,json",
    "ignore": [
      "node_modules/",
      "__tests__/",
      "public/",
      "docs/"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!__tests__/**",
      "!seeds/**",
      "!public/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80,
    "arrowParens": "always"
  }
}
